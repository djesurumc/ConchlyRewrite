{
  "expo": {
    "name": "Conchly",
    "slug": "Conchly",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./app/assets/branding/icon.png",
    "splash": {
      "image": "./app/assets/branding/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/branding/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/branding/favicon.png"
    }
  }
}
